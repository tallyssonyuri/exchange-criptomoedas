package main;

import javax.swing.SwingUtilities;

public class Main {
    public static void main(String[] args) {
        SwingUtilities.invokeLater(() -> {
            SistemaCriptoGUI gui = new SistemaCriptoGUI();
            
            gui.setVisible(true);
            
            gui.setLocationRelativeTo(null);
        });
    }
}
